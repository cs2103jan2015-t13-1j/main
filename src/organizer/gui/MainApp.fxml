<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.shape.*?>
<?import java.net.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.input.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.net.* ?>

<AnchorPane minWidth="-Infinity" prefHeight="617.0" prefWidth="1078.0" styleClass="root" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="organizer.gui.MainAppController">
   <stylesheets>
      <URL value="@mainstyle.css" />
   </stylesheets>
   <children>
      <HBox prefHeight="31.0" prefWidth="1078.0">
         <children>
            <MenuBar HBox.hgrow="ALWAYS">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Save and Exit">
                           <accelerator>
                              <KeyCodeCombination alt="UP" code="E" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                           </accelerator>
                        </MenuItem>
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" onAction="#openDialogHelp" text="Manual" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </children>
      </HBox>
      <FlowPane fx:id="deadlineTaskPane" hgap="15.0" layoutX="27.0" layoutY="85.0" prefHeight="479.0" prefWidth="682.0" vgap="15.0">
         <padding>
            <Insets bottom="30.0" left="10.0" right="10.0" top="30.0" />
         </padding>
         <styleClass>
            <String fx:value="deadlinetaskpane" />
            <String fx:value="pane" />
         </styleClass></FlowPane>
      <FlowPane fx:id="floatingTaskPane" layoutX="754.0" layoutY="85.0" prefHeight="448.0" prefWidth="258.0" vgap="15.0">
         <padding>
            <Insets top="30.0" />
         </padding>
      </FlowPane>
      <TextField fx:id="commandText" layoutX="27.0" layoutY="47.0" onAction="#performCommand" onKeyPressed="#keyPressHandler" prefHeight="31.0" prefWidth="1015.0" styleClass="command-input" />
      <Label id="commandStatus" fx:id="commandStatus" layoutX="903.0" layoutY="585.0" text="Command result" />
      <Label fx:id="pageStatus" layoutX="27.0" layoutY="570.0" styleClass="circle-text" text="0 of 0" wrapText="true">
         <padding>
            <Insets left="10.0" />
         </padding>
      </Label>
   </children>
</AnchorPane>
